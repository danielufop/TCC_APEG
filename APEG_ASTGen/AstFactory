class AstFactory(ASTNode): 
	properties = dict(and = "AndExprNode", andPeg = "AndPegNode", anyPeg = "AnyPegNode", assign = 					"AssignmentNode", attri = "AttributeExprNode", binaExpr = "BinaryExprNode", 
				bindPeg = "BindPegNode", booleanExpr = "BooleanExprNode", typeNode = "TypeNode", 					callExpr = "CallExprNode", choicePeg = "ChoicePegNode", constPeg = 					"ConstraintPegNode", equalExpr = "EqualityExprNode", floatExpr = "FloatExprNode", 					 grammar = "GrammarNode", groupPeg = "GroupPegNode", intExpr = "IntExprNode", 					lambdaPeg = "LambdaPegNode", litPeg = "LiteralPegNode", metaPeg = 					"MetaPegExprNode", minusExpr = "MinusExprNode", nontermPeg = "NonterminalPegNode", 					notExpr = "NotExprNode", notPeg = "NotPegNode", optionPeg = "OptionalPegNode", 					orExpr = "OrExprNode", plusPeg = "PlusPegNode", rule = "RuleNode", seqPeg = 					"SequencePegNode", starPeg = "StarPegNode", stringExpr = "StringExprNode", 					updatePeg = "UpdatePegNode", varDeclarations = "VarDeclarations"
